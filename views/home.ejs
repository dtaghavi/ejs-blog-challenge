<%- include("partials/header") -%>
<h1>Home</h1>
<p><%= startingContent %></p>
<% postsArray.forEach(function(post) {%>

  <h1 class="post-title"><%= post.title %></h1>
  <p class="post-content">
    <%= post.content.substring(0, 100) + "..." %> 
    <a href="/post/<%=post.title%>">Read More</a>
  </p>

<%})%>
<%- include("partials/footer") -%>
